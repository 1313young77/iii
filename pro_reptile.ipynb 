   "source": [
    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import time\n",
    "from math import cos\n",
    "\n",
    "res_1 = requests.get('http://www.ipeen.com.tw/search/taipei/000/4-9-45-13/')\n",
    "soup = BeautifulSoup(res_1.text,'html.parser')\n",
    "entertainment_names = soup.findAll('a',{'data-label':'店名'})\n",
    "entertainment_address = soup.findAll('span',{'style':'padding-left:3em;'})\n",
    "        \n",
    "entertainment_names_list_first = []\n",
    "entertainment_names_list_first.append(entertainment_names)\n",
    "entertainment_address_list_first = []\n",
    "entertainment_address_list_first.append(entertainment_address)\n",
    "      \n",
    "i = 2\n",
    "entertainment_names_list = []\n",
    "entertainment_address_list = []\n",
    "\n",
    "while i <= 7:\n",
    "    url_2 = 'http://www.ipeen.com.tw/search/taipei/000/4-9-45-13/?p='\n",
    "    page = str(i)\n",
    "    web = url_2 + page\n",
    "    \n",
    "    res_2 = requests.get(web)\n",
    "    soup = BeautifulSoup(res_2.text,\"html.parser\")\n",
    "    entertainment_names = soup.findAll('a',{'data-label':'店名'})\n",
    "    entertainment_address = soup.findAll('span',{'style':'padding-left:3em;'})\n",
    "\n",
    "    for n in entertainment_names:\n",
    "        name = n.get_text()\n",
    "        entertainment_names_list.append(name)\n",
    "    \n",
    "    for a in entertainment_address:\n",
    "        address = a.get_text()\n",
    "        entertainment_address_list.append(address.split()[0])\n",
    "\n",
    "    i+=1\n",
    "    \n",
    "entertainment_names_list.extend(entertainment_names_list_first)\n",
    "entertainment_address_list.extend(entertainment_address_list_first)\n",
    "\n",
    "data = {'name':entertainment_names_list, 'address':entertainment_address_list}\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "for i in range(len(df['address'])):\n",
    "    df['address'][i] = str(df['address'][i]).split('號')[0] + '號'\n",
    "\n",
    "key1 = 'AIzaSyC_vcarGfMvLtfT9Hzgn1Q8ZgUbShHDSjk'\n",
    "\n",
    "def geoc(df):\n",
    "    df['lan'] = pd.Series(np.zeros(len(df)), index=df.index)\n",
    "    df['long'] = pd.Series(np.zeros(len(df)), index=df.index)\n",
    "    \n",
    "    total = 0\n",
    "\n",
    "    for i in range(len(df)):\n",
    "        print(i)\n",
    "        total+=1\n",
    "        addr = df['address'][i]\n",
    "        url = 'https://maps.googleapis.com/maps/api/geocode/xml?address=' + addr + '&key=' + key1\n",
    "        r = requests.get(url)\n",
    "        content = r.content\n",
    "        bsobj = BeautifulSoup(content, 'html.parser')\n",
    "        status = bsobj.find('status').get_text()\n",
    "        if status == 'OVER_QUERY_LIMIT':\n",
    "            print('need to change key, the current number = ' + str(i))\n",
    "            df.to_csv('house_temp.csv', index=False)\n",
    "            break\n",
    "        elif status == 'OK':\n",
    "            lan = bsobj.find_all('lat')[0].get_text()    \n",
    "            long = bsobj.find_all('lng')[0].get_text()\n",
    "        else:\n",
    "            print('address is not vlaid')\n",
    "            lan = 0\n",
    "            long = 0\n",
    "        df['lan'][i] = lan\n",
    "        df['long'][i] = long\n",
    "        print('緯度為: ' + str(lan))\n",
    "        print('經度為: ' + str(long))\n",
    "        \n",
    "        time.sleep(1)\n",
    "    return df\n",
    "\n",
    "testdf = geoc(df)\n",
    "\n",
    "done_well = testdf\n",
    "def state_code_compute(x):\n",
    "    scale_km = 0.5\n",
    "    lan = x['lan']\n",
    "    long = x['long']\n",
    "    lat_to_km = lan*110.574\n",
    "    lon_to_km = abs(long*(111.320*cos(lan)))\n",
    "\n",
    "    state_code_x = str(int(lat_to_km//scale_km))\n",
    "    state_code_y = str(int(lon_to_km//scale_km))\n",
    "    if len(state_code_y) == 4:\n",
    "        state_code_y = '0'+state_code_y\n",
    "    state_code = state_code_x+state_code_y\n",
    "    return state_code\n",
    "\n",
    "done_well['state_code'] = done_well.apply(state_code_compute, axis = 1)\n",
    "# done_well.to_csv('done_well.csv',index= False)"
   ]
